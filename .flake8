[flake8]
max-line-length = 100
exclude = 
    .git,
    __pycache__,
    build,
    dist,
    logs,
    temp_uploads,
    *.egg-info,
    .venv/Lib/site-packages/sympy/
ignore = E203,W503,E501,C901,W293
per-file-ignores =
    __init__.py: F401,F403
    log_llm.py: E402,F405,F403,E722,F824,F541,F811
    llm1.py: E402,F405,F403,E722,F824,F541,F811
    test_*.py: F401,F811
max-complexity = 18